# Threat Intelligence Feed Aggregator - Environment Configuration Template
# Copy this file to .env and add your actual API keys

# Google Gemini API Keys (Get from: https://makersuite.google.com/app/apikey)
GEMINI_API_KEY_1=your-first-gemini-api-key-here
GEMINI_API_KEY_2=your-second-gemini-api-key-here

# AI Configuration
AI_PROVIDER=gemini

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=7860

# Application Settings
APP_TITLE="Threat Intelligence Feed Aggregator"
APP_DESCRIPTION="AI-powered threat intelligence aggregation and analysis platform"

# Processing Limits
MAX_ITEMS_PER_FEED=20
MAX_RECENT_THREATS=50
MAX_SEARCH_RESULTS=50
MAX_EXPORT_ITEMS=100

# Auto-refresh interval (seconds)
AUTO_REFRESH_INTERVAL=300

# Database
DATABASE_PATH=threat_intel.db

# AI Request Configuration
AI_REQUEST_TIMEOUT=45
AI_MAX_RETRIES=5
AI_RETRY_DELAY=1
MAX_CONCURRENT_AI_REQUESTS=6

# Rate Limiting
RATE_LIMIT_WINDOW=60
